{
  "ConnectionStrings" : {
     "HeartbeatContext" :  #{MYSQL_CONNECTION_STRING},
     "InfluxDBContext" : #{INFLUXDB_TOKEN}
  },
  "Rabbit": {
    "ClientDeclarations": {
      "Connections": [
        {
          "Name": "LifeAlertConnection",
          "ClientName": "LifeAlert",
          "ServerName": "rabbit.centurionx.local",
          "UserName": "general",
          "Password": #{RABBIT_PASSWORD}
        }
      ],
      "AsyncRecievers": [
        {
          "Name": "IncomingMessages",
          "Connection": "LifeAlertConnection",
          "Queue": "life_alert_timing"
        }
      ]
    },
    "ServerDeclarations": {
      "Exchanges": [
        {
          "Name": "Clock",
          "Type": "Topic",
          "AutoDelete": "False",
          "Durable": "True",
          "Connection": "LifeAlertConnection"
        }
      ],
      "Bindings": [
        {
          "Queue": "life_alert_timing_dev",
          "Connection": "LifeAlertConnection",
          "Exchange": "Clock",
          "SubscriptionKey": "tick.minute"
        }
      ],
      "Queues": [
        {
          "Name": "life_alert_timing",
          "Connection": "LifeAlertConnection",
          "AutoDelete": "true",
          "Durable": "false",
          "Exclusive": "false"
        }
      ]
    }
  }
  "Rate":3
}