{
    "Application":
    {
        "Name": "MetricMonitor",
        "Version": "5.5.5.5"
    },
    "ConnectionStrings": 
    {
      "InfluxDBContext" : #{INFLUXDB_TOKEN}
    },
    "Rabbit":
    {
        "Scale": 1,
        "ClientDeclarations": 
        {
            "Connections":
            [
                {
                    "Name":"MetricsConnection",
                    "ClientName": "MetricMonitor",
                    "ServerName": "10.0.0.215",
                    "UserName": "general",
                    "Password": "#{RABBIT_PASSWORD}"
                }
            ],
            "AsyncRecievers":
            [
                {
                    "Name": "IncomingMessages",
                    "Connection": "MetricsConnection",
                    "Queue": "metric"
                }
            ]
        },
        "ServerDeclarations":
        {
            "Exchanges":
            [
                {
                    "Name": "InterTopic",
                    "Type": "Topic",
                    "AutoDelete": "False",
                    "Durable": "True",
                    "Connection": "MetricsConnection"
                }
            ],
            "Bindings":
            [
                {
                    "Queue": "metric",
                    "Connection": "MetricsConnection",
                    "Exchange": "InterTopic",
                    "SubscriptionKey": "metric"
                }
            ],
            "Queues":
            [
                {
                    "Name": "metric",
                    "Connection": "MetricsConnection",
                    "AutoDelete": "False",
                    "Durable": "True",
                    "Exclusive": "False"
                }
            ]
        } 
    }
}