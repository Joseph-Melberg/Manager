{
  "ConnectionStrings" : {
    "ReadWriteContext" : #{READWRITE_MYSQL},
    "PlaneCacheContext" : #{ONPREM_REDIS}
  },
  "Rabbit" :
  {
    "ClientDeclarations": 
    {
      "Connections":
      [
        {
          "Name":"PlaneConnection",
          "ClientName": "PlaneListener",
          "ServerName": "rabbit.centurionx.local",
          "UserName": "planeysecure",
          "Password": #{PLANEYSECURE_PASSWORD}
        }
      ],
      "AsyncRecievers":
      [
        {
          "Name": "IncomingMessages",
          "Connection": "PlaneConnection",
          "Queue": "plane"
        }
      ]
    },
    "ServerDeclarations":
    {
      "Exchanges":
      [
        {
          "Name": "InterTopic",
          "Type": "Topic",
          "AutoDelete": "False",
          "Durable": "True",
          "Connection": "PlaneConnection"
        }
      ],
      "Bindings":
      [
        {
          "Queue": "plane",
          "Connection": "PlaneConnection",
          "Exchange": "InterTopic",
          "SubscriptionKey": "plane.final"
        }
      ],
      "Queues":
      [
        {
          "Name": "plane",
          "Connection": "PlaneConnection",
          "AutoDelete": "False",
          "Durable": "True",
          "Exclusive": "False"
        }
      ]
    } 
  }
}
