{
    "Application":
    {
        "Name": "PlaneCongregator",
        "Version": "5.5.5.5"
    },
    "ConnectionStrings": 
    {
        "PlaneCacheContext" : #{ONPREM_REDIS},
        "InfluxDBContext" : #{INFLUXDB_TOKEN}
    },
    "Rabbit" :
    {
      "Scale":1,
      "ClientDeclarations": 
      {
        "Connections":
            [
                {
                    "Name":"TickConnection",
                    "ClientName": "PlaneCongregator",
                    "ServerName": "10.0.0.215",
                    "UserName": "general",
                    "Password": "#{RABBIT_PASSWORD}"
                },
                {
                    "Name":"MetricConnection",
                    "ClientName": "CpuMonitor",
                    "ServerName": "10.0.0.215",
                    "UserName": "general",
                    "Password": "#{RABBIT_PASSWORD}"
                }
            ],
            "AsyncRecievers":
            [
                {
                    "Name": "IncomingMessages",
                    "Connection": "TickConnection",
                    "Queue": "plane_congregator_clock"
                }
            ],
            "Publishers":
            [
                {
                "Name": "MetricMessage",
                "Connection": "PlaneConnection",
                "Exchange": "InterTopic"
                }
            ]
        },
        "ServerDeclarations":
        {
            "Exchanges":
            [
                {
                    "Name": "Clock",
                    "Type": "Topic",
                    "AutoDelete": "False",
                    "Durable": "True",
                    "Connection": "TickConnection"
                }
            ],
            "Bindings":
            [
                {
                    "Queue": "plane_congregator_clock",
                    "Connection": "TickConnection",
                    "Exchange": "Clock",
                    "SubscriptionKey": "tick.second"
                }
            ],
            "Queues":
            [
                {
                    "Name": "plane_congregator_clock",
                    "Connection": "TickConnection",
                    "AutoDelete": "false",
                    "Durable": "false",
                    "Exclusive": "False"
                }
            ]
        } 
    }
}
