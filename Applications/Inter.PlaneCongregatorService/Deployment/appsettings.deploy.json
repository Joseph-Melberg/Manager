{
    "ConnectionStrings": 
    {
        "PlaneCacheContext" : #{ONPREM_REDIS},
        "InfluxDBContext" : #{INFLUXDB_TOKEN}
    },
    "Rabbit" :
    {
      "ClientDeclarations": 
      {
        "Connections":
            [
                {
                    "Name":"TickConnection",
                    "ClientName": "PlaneCongregator",
                    "ServerName": "rabbit.centurionx.local",
                    "UserName": "general",
                    "Password": "#{RABBIT_PASSWORD}"
                }
            ],
            "AsyncRecievers":
            [
                {
                    "Name": "IncomingMessages",
                    "Connection": "TickConnection",
                    "Queue": "plane_congregator_clock"
                }
            ]
        },
        "ServerDeclarations":
        {
            "Exchanges":
            [
                {
                    "Name": "Clock",
                    "Type": "Topic",
                    "AutoDelete": "False",
                    "Durable": "True",
                    "Connection": "TickConnection"
                }
            ],
            "Bindings":
            [
                {
                    "Queue": "plane_congregator_clock",
                    "Connection": "TickConnection",
                    "Exchange": "Clock",
                    "SubscriptionKey": "tick.second"
                }
            ],
            "Queues":
            [
                {
                    "Name": "plane_congregator_clock",
                    "Connection": "TickConnection",
                    "AutoDelete": "false",
                    "Durable": "false",
                    "Exclusive": "False"
                }
            ]
        } 
    }
}
